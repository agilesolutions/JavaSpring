{
  "id": null,
  "title": "Spring Boot Performance Metrics",
  "tags": ["springboot", "micrometer", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "JVM Heap Memory Used",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "Used Heap"
        },
        {
          "expr": "jvm_memory_max_bytes{area=\"heap\"}",
          "legendFormat": "Max Heap"
        }
      ],
      "id": 1
    },
    {
      "type": "timeseries",
      "title": "CPU Utilization",
      "targets": [
        {
          "expr": "system_cpu_usage",
          "legendFormat": "System CPU Usage"
        },
        {
          "expr": "process_cpu_usage",
          "legendFormat": "Process CPU Usage"
        }
      ],
      "id": 2
    },
    {
      "type": "stat",
      "title": "Active HTTP Requests (RPS)",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[1m]))",
          "legendFormat": "Requests/sec"
        }
      ],
      "id": 3
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency (95th Percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95 Latency"
        }
      ],
      "id": 4
    }
  ],
  "templating": {
    "list": [
      {
        "name": "application",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(jvm_memory_used_bytes, application)",
        "refresh": 1,
        "includeAll": true,
        "multi": true
      }
    ]
  }
}
